{
  "manifest_version": 2,
  "name": "Leboncoin ad reload",
  "description": "Publish your leboncoin ad again in one click",
  "version": "0.0.0",
  "permissions": [
    "https://www.leboncoin.fr/*",
    "https://compteperso.leboncoin.fr/*",
    "activeTab"
  ],
  "content_scripts": [ {
    "js": ["./node_modules/jquery/dist/jquery.min.js"],
    "matches": [ "https://www.leboncoin.fr/*","https://compteperso.leboncoin.fr/*" ]
  }],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128" : "128.png"
  }
}
